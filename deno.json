{
  "tasks": {
    "dev": "deno run --watch main.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/preact@10.16.0",
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns"]
},
 "lint": {
        "rules": {
          "tags": ["recommended"],
          "include": ["ban-untagged-todo","no-unused-vars"],
          "exclude": ["no-explicit-any"]
        }
    },
  "lock": false,
  "unstable": ["worker-options"],
  "imports": {
  "$/": "./",
  "$std/": "https://deno.land/std@0.208.0/",
  "$puppeteer2": "jsr:@astral/astral",
  "$puppeteer": "../astral/mod.ts",
  "LaunchOptions": "https://deno.land/x/puppeteer@16.2.0/src/deno/LaunchOptions.ts",
  "$ignore": "jsr:@deno-library/progress@^1.5.1",

    "@deno-library/progress": "jsr:@deno-library/progress@^1.5.1",
    "@std/assert": "jsr:@std/assert@^1",
    "@std/async": "jsr:@std/async@^1",
    "@std/fs": "jsr:@std/fs@^1",
    "@std/path": "jsr:@std/path@^1",
    "@std/testing": "jsr:@std/testing@^1",
    "@std/encoding": "jsr:@std/encoding@1",
    "@zip-js/zip-js": "jsr:@zip-js/zip-js@^2.7.52",
    "@deno/cache-dir": "jsr:@deno/cache-dir@0.22.2"
  }
}
